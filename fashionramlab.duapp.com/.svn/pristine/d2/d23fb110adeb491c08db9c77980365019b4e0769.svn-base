function findListTag(){
    var mpid=$("#mpid").val();
    $.ajax({
        type:'GET',
        url: '/mptags/'+mpid,
        success:function(date){
           arrfun(date);
        }
    });
}

function arrfun(date){
  $(date).find("tags").each(function(){
          var aid = $(this).children("tag").attr("id");
          var aname = $(this).children("tag").attr("name");
          alert(aid+":hello:"+aname)
          $(this).children("tag").each(function(){
             aid = $(this).children("tag").attr("id");
             aname = $(this).children("tag").attr("name");
             $("#body").append("<tr>" +
                 "<td>" +aname+"</td>" +
                 "<td>" +aid+"</td>" +
                 "</tr>"
             )

          });

	});



}