
<div class="container-fluid">
		<div class="row-fluid">
			<div class="row-fluid sortable">
				<div class="box span12">
						<div class="box-header well" data-original-title>
						<div class="box-icon">
							<button type="button" class="btn" onclick="showconsole_module_cms();">添加资讯</button>
						</div>
						</div>
                      <div style="margin-left:10px;margin-top: 15px;"><span>分类</span>&nbsp;&nbsp;
                          <select class="tagList" onchange="changeForm()"><option value="全部">全部</option></select>
                        </div>

						<div class="box-content">
							<table class="table table-striped table-bordered bootstrap-datatable datatable" id="mparticlelist">
							  <tr id="titlist">
							      <th>标题</th>
                                  <th>时间</th>
								  <th>优先级</th>
                                  <th>概述</th>
                                  <th>缩略图</th>
								  <th>标签</th>
                                  <th>操作</th>
							  </tr>
                              <tbody id="body">

						      </tbody>
							</table>
						</div>
				</div>

			</div>
			</div>
</div>
 <div id="dialog-console_module_cms" title="新增" style="display:none">

	<div style="displacy:inline-block">标签<input type="text" style="width: 300px;height: 30px;" id="tag_input" onChange="add_tag(this)"/></div>
    <label>标题<input type="text" id="title" style="width: 300px;height: 30px;"/></label>
    <label>优先级:<input type="text" id="priority"  value="1" style="width: 300px;height: 30px;"/></label>
	<label>常用标签:<select class="tagList"  onchange="add_tag_text($('#tag_input'),$(this).find('option:selected').text())"></select></label>
	<label>概述:<input type="text" id="summary"  style="width: 300px;height: 30px;"/></label>
    <label>缩略图:<input type="text" id="thumb"  style="width: 300px;height: 30px;"/></label>
    <label>授权<input type="text" id="enabled"  style="width: 300px;height: 30px;"/></label>
    <form name="form"  method="POST" enctype="multipart/form-data">
          <input type="file" name="file"  id="file"/><input type="button" value="上传" onclick="ajaxFileUpload()"/>
    </form>
    <span id="imagesShow"></span>
    <label>图片<select id="images"><input type="button" value="插入" onclick="addsection();" ></select></label>
	<textarea name="section1" id="content"></textarea>
    <center id="addcms"  style="color: red; font-size:20px;"></center>
</div>

 <div id="dialog-console_module_update" style="display:none">
    <div style="displacy:inline-block">标签<input type="text" style="width: 300px;height: 30px;" id="prcetag" onChange="add_tag(this)"/></div>
	<label>标题<input type="text" id="updatitle" style="width: 300px;height: 30px;"/></label>
    <label>优先级<input type="text" id="updatpriority" style="width: 300px;height: 30px;"/></label>
    <label>概述<input type="text" id="updatesummary"  style="width: 300px;height: 30px;"/></label>
    <form name="form"  method="POST" enctype="multipart/form-data">
          <input type="file" name="file"  id="sectionImage"/><input type="button" value="上传" onclick="ajaxFileUpload2()"/>
    </form>
    <span id="sectionShow"></span>
    <label>图片<select id="addsection"><input type="button" value="插入" onclick="addsection2();" ></select></label>
	<textarea name="section2"></textarea>
    <center id="upatacms"  style="color: red; font-size:20px;"></center>
 </div>
<script type="text/javascript"> get_tag_list();

    mparticlesmpid();  //mpid 搜索列表
    findListIamges();
    addsectionListIamges();
    $("#dialog-console_module_cms" ).dialog({
      resizable: false,
	  autoOpen: false,
      height:680,
	  width: 650,
      modal: true,
      buttons: {
        "添加": function() {
        addconsole_module_cms();
         findconsole_module_cms();
        }
      }
    });


</script>
