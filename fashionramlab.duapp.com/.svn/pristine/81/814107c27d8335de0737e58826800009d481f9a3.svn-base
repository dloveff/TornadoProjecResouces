function findListTag(){
    var mpid=$("#mpid").val();
    $.ajax({
        type:'GET',
        url: '/mptags/'+mpid,
        success:function(date){
           arrfun(date);
        }
    });
}

function arrfun(date){
  $(date).find("tags").each(function(){

          $(this).children("tag").each(function(){
             var id = $(this).children("tag").attr("id");
             var name = $(this).children("tag").attr("name");
               $("#body").append("<tr>" +
                   "<td>" +name+"</td>" +
                   "<td>" +id+"</td>" +
                   "</tr>")

          });

	});



}