<script type="text/javascript">
 $(function(){
	$.ajax({
		   type:'GET',
		   url: '/currentuser',
		   async:false,
		   success:function(date){
		       	var role=$(date).find("user").children("role").text();
					if(role=="admin"){
						  $("#account").css("display","block");
						 $("#sytem").css("display","block");
					
					}	
		    	},error: function(XMLHttpRequest, textStatus, errorThrown) {
                     $("#account").css("display","none");
					  $("#sytem").css("display","none");
                    }

		   });
});

$(function(){
　 $("#find").load("myconsole_users.html");
});
</script>
<script type="text/javascript">
function logout(){
	$.ajax({
		   type:'POST',
		   url: '/logout',
		   async:false,
		   success:function(date){
		      location.href="login.html";			
		    	}

		   });
}
$(document).ready(
   function(){
      $("a[target]").click(
            function(){
				tg = $(this).attr('target');
				h = $(this).attr('href');
                 $(tg).empty().load(h);
                   return false;
               });
        });

</script>

