function findListTag(){
var mpid=$("#mpid").val();
$.ajax({
		   type:'GET',
		   url: '/mptags/'+mpid,
		   success:function(date){
                    arrfun(date);
		    	}
		   });
}

function arrfun(date){
  $(date).find("tags").each(function(){
          $(this).children("tag").each(function(){
                     alert($(this).attr("name"))

          });

	});



}