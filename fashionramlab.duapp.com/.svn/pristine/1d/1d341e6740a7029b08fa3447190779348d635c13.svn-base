function service_to_form(xml, frm) {
    $(':eq(0)', xml).each(function () {
        $.each(this.attributes, function () {
            if(this.specified) {
                var name = this.name;
                var value = this.value;
                $('[name="' + name + '"]', frm).val(value);
            }
        });
    });
    $(':eq(0)', xml).children().each(function () {
        var name = $(this).prop('tagName');
        var value = $(this).text();
        $('[name="' + name + '"]', frm).val(value);
    });
}