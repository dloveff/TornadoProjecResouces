function findListTag(){
    var mpid=$("#mpid").val();
    $.ajax({
        type:'GET',
        url: '/mptags/'+mpid,
        success:function(date){
           arrfun(date);
        }
    });
}



function arrfun(date){
   $(date).children("tags").children("tag").each(function(){
          var aid = $(this).children("tag").attr("id");
          var aname = $(this).children("tag").attr("name");
          addelement(aname,aid);
          $(this).find("tag").each(function(){
              var id = $(this).children("tag").attr("id");
              var name = $(this).children("tag").attr("name");
              addelement(name,id)
          })
  });
}
function add_tag(){
     $("#dialog-mp_tog_add" ).dialog({
          resizable: false,
          autoOpen: false,
          height:680,
          width: 650,
          modal: true,
          buttons: {
            "添加": function() {
             submit_add_tag()
            }
          }
      });
}
function submit_add_tag(){
    $("#tag_name").val()
}
function addelement(uname,uid){
    $("#body").append("<tr><td>" + uname +"</td><td>" + uid +"</td></tr>")
}