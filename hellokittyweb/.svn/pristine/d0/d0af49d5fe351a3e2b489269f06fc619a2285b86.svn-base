      <article class="oe_page" align="center">
    <section class="oe_container" align="center">
        <div class="oe_row oe_spaced" align="center">
            <h2 class="oe_slogan">联系我们</h2>
            <div class="oe_contact_thanks">
                
            </div>

            <form action="" method="get" class="oe_contact_form">
                <table class='oe_form_layout_table'>
                    <tr>
                        <td>
                            <label class="oe_input_label">您的电子邮件地址</label>
                        </td>
                        <td>
                            <input class='oe_input' type="text" name="email" autofocus="autofocus"/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label class="oe_input_label">您的全名</label>
                        </td>
                        <td>
                            <input class='oe_input' type="text" name="name"/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label class="oe_input_label">您的公司</label>
                        </td>
                        <td>
                            <input class='oe_input' type="text" name="company"/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label class="oe_input_label">您的电话号码（可选）</label>
                        </td>
                        <td>
                            <input class='oe_input' type="text" name="phone"/>
                        </td>
                    </tr>
                    <tr>
                        <td>
                            <label class="oe_input_label">你的主要兴趣</label>
                        </td>
                        <td>
                            <select name="plan" class='oe_input'>
                                <option value="qt">其它</option>
                                <option value="hz">想要合作</option>
                                <option value="yw">业务咨询</option>
                                <option value="jy">建议/意见</option>
                            </select>
                        </td>
                    </tr>
                    <tr>
     <td>
     <label class="oe_input_label">您的问题/评论</label>
     </td>
     <td>
     <textarea name="text" class='oe_input oe_textarea'></textarea>
     </td>
                    </tr>
                    <tr>
                        <td></td>
                        <td>
                            <button name="submit" class='oe_button'>
                                发送我的问题
                                <i class="icon-spinner icon-spin icon-large" style="display: none;"></i>
                            </button>
                        </td>
                    </tr>
                </table>
            </form>
